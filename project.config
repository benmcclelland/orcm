[project]
	description = Open Resilient Cluster Manager
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Ben McClelland (benmcclelland)
[access "refs/for/refs/*"]
	read = group user/Ben McClelland (benmcclelland)
	push = group user/Ben McClelland (benmcclelland)
	label-Code-Review = -2..+2 group user/Ben McClelland (benmcclelland)
	label-Verified = -1..+1 group user/Ben McClelland (benmcclelland)
	rebase = group user/Ben McClelland (benmcclelland)
	removeReviewer = group user/Ben McClelland (benmcclelland)
	submit = group user/Ben McClelland (benmcclelland)
[access "refs/heads/*"]
	read = group user/Ben McClelland (benmcclelland)
	create = group user/Ben McClelland (benmcclelland)
	push = +force group user/Ben McClelland (benmcclelland)
	pushMerge = group user/Ben McClelland (benmcclelland)
[access "refs/tags/*"]
	push = +force group user/Ben McClelland (benmcclelland)
	pushTag = group user/Ben McClelland (benmcclelland)
	pushSignedTag = group user/Ben McClelland (benmcclelland)
